{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "hasura-admin",
  "title": "Hasura Admin",
  "description": "Search hasura actions and tables blazingly fast",
  "icon": "command-icon.png",
  "author": "gebeto",
  "categories": [
    "Web",
    "Developer Tools",
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Hasura Admin",
      "subtitle": "Hasura",
      "description": "Template for showing typeahead search with network results.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.35.2",
    "node-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}